{
  "name": "ar-cascader",
  "version": "0.0.1",
  "description": "administrative region cascader base on view-ui-plus, vue3 version",
  "type": "module",
  "files": [
    "dist",
    "src",
    "examples",
    "types"
  ],
  "main": "./dist/ar-cascader.umd.cjs",
  "module": "./dist/ar-cascader.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ar-cascader.js",
      "require": "./dist/ar-cascader.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib"
  },
  "dependencies": {
    "area-data": "^5.0.6",
    "view-ui-plus": "^1.3.14",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}
