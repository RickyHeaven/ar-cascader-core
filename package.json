{
  "name": "ar-cascader",
  "version": "2.1.1",
  "description": "regionalism cascader base on view-ui-plus, vue3 version，中国行政区域区域划分(含港澳台)级联选择器，三级，省-市-区，数据来源于国家统计局2023年6月统计用区划代码",
  "type": "module",
  "files": [
    "dist",
    "src",
    "examples",
    "types"
  ],
  "main": "./dist/ar-cascader.umd.cjs",
  "module": "./dist/ar-cascader.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ar-cascader.js",
      "require": "./dist/ar-cascader.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/ar-cascader.git"
  },
  "keywords": [
    "administrative",
    "region",
    "cascader",
    "ar",
    "ricky",
    "zhangqingcq",
    "vue",
    "view-design",
    "area-data",
    "China",
    "行政区域代码",
    "中国省份",
    "行政区域级联"
  ],
  "author": "RickyHeaven",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RickyHeaven/ar-cascader-core/issues"
  },
  "homepage": "https://github.com/RickyHeaven/ar-cascader-core#readme",
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@zhangqingcq/china-area-data": "^0.0.3",
    "lodash-es": "^4.17.21",
    "view-ui-plus": "^1.3.14",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "engines": {
    "node": ">=20.3.1",
    "npm": ">=9.6.7",
    "pnpm": ">=8.6.7"
  }
}
